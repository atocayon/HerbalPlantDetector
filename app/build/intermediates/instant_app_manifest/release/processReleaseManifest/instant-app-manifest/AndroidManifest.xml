<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.com.tflitecamerademo"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:icon="@drawable/pitahc_logo"
        android:label="DOH Herbal Plants Detector"
        android:roundIcon="@drawable/pitahc_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
        <activity android:name="com.herbal.DOH.tflitecamerademo.Project_Info" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.Developers" />
        <activity
            android:name="com.herbal.DOH.tflitecamerademo.CameraActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.herbal.DOH.tflitecamerademo.YerbaBuena" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.Bayabas" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.Bawang" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.UlasimangBato" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.Lagundi" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.Ampalaya" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.TsaangGubat" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.NiyogNiyogan" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.Akapulko" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.Sambong" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.Manual" />
        <activity android:name="com.herbal.DOH.tflitecamerademo.About" />
    </application>

</manifest>